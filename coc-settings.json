    "border": true,
    "rounded": true
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.floatConfig.border": true,
  "suggest.floatConfig.rounded": true,
  "suggest.floatConfig.borderhighlight": "CocFloating",
  "suggest.floatConfig.highlight": "CocFloatingBackground",
  "suggest.floatConfig.winblend": 5,
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
